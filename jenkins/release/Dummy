@Library('reanplatform') _

node {
    pythonPipelines.releaseBuild(
        name: 'reanplatform-cli',
        setup_py_path: 'REANPlatform',
        autoRelease: false
    )

    withCredentials([string(credentialsId: 'artifactory-api-key', variable: 'ARTIFACTORY_API_KEY')) {
         sh "#!/bin/bash -l\nbundle exec  rake document:generate"
        }

}
